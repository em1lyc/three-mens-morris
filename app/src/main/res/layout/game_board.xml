<!-- Gameboard -->
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/gameboard"
    android:layout_width="208dp"
    android:layout_height="208dp"
    android:layout_centerInParent="true"
    android:background="@drawable/gameboard">
    <!-- Board -->
    <View
        android:id="@+id/board"
        android:layout_width="193dp"
        android:layout_height="193dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="7dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="7dp"
        android:background="@drawable/board" />
    <!-- Row -->
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/row0"
        android:layout_width="207dp"
        android:layout_height="15dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/row">
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/blank_circle00"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentStart="true"
            android:background="@drawable/blank_circle"
            />
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/blank_circle01"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_centerInParent="true"
            android:background="@drawable/blank_circle"
            />
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/blank_circle02"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:background="@drawable/blank_circle"
            />
    </RelativeLayout>
    <!-- Row -->
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/row1"
        android:layout_width="207dp"
        android:layout_height="15dp"
        android:layout_centerInParent="true"
        android:background="@drawable/row">
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/blank_circle10"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentStart="true"
            android:background="@drawable/blank_circle"
            />
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/blank_circle11"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_centerInParent="true"
            android:background="@drawable/blank_circle"
            />
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/blank_circle12"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:background="@drawable/blank_circle"
            />
    </RelativeLayout>
    <!-- Row -->
    <RelativeLayout
        android:id="@+id/row2"
        android:layout_width="207dp"
        android:layout_height="15dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/row">
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/blank_circle20"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentStart="true"
            android:background="@drawable/blank_circle"
            />

        <RelativeLayout
            android:id="@+id/blank_circle21"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_centerInParent="true"
            android:background="@drawable/blank_circle" />
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/blank_circle22"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentEnd="true"
            android:background="@drawable/blank_circle"
            />
    </RelativeLayout>
</RelativeLayout>